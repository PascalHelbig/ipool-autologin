{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://ipool.ba-berlin.de/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "scripts/ipool-autologin.user.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "http://ipool.ba-berlin.de/*"
  ]
}
